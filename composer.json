{
  "name": "popkovkirill/nutgram-spiral",
  "description": "This is my package nutgram-spiral",
  "keywords": [
    "Nutgram",
    "spiral",
    "nutgram-spiral"
  ],
  "homepage": "https://github.com/popkovkirill/nutgram-spiral",
  "license": "MIT",
  "authors": [
    {
      "name": "Kirill Popkov",
      "email": "kirill.popkov.work@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.4",
    "nutgram/nutgram": "^4.38",
    "spiral/boot": "^3.7",
    "spiral/console": "^3.7"
  },
  "require-dev": {
    "mockery/mockery": "^1.5",
    "phpunit/phpunit": "^10.1",
    "spiral/code-style": "^2.2",
    "spiral/router": "^3.15",
    "spiral/testing": "^2.0",
    "vimeo/psalm": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "Nutgram\\Spiral\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nutgram\\Spiral\\Tests\\App\\": "tests/app",
      "Nutgram\\Spiral\\Tests\\": "tests/src"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "cs:diff": "php-cs-fixer fix --dry-run -v --diff",
    "cs:fix": "php-cs-fixer fix -v",
    "psalm": "psalm",
    "psalm:baseline": "psalm --set-baseline=psalm-baseline.xml"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "spiral": {
      "bootloaders": [
        "Nutgram\\Spiral\\Bootloader\\NutgramBootloader"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
